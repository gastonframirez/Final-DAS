<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<jaxws:endpoint
		xmlns:tns="http://ws.comercio.src.das.ubp.edu.ar/" id="ofertasws"
		implementor="ar.edu.ubp.das.src.comercio.ws.OfertasWS"
		wsdlLocation="wsdl/ofertasws.wsdl" endpointName="tns:OfertasWSPort"
		serviceName="tns:OfertasWSService" address="/OfertasWSPort">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
	<jaxws:endpoint
		xmlns:tns="http://ws.comercio.src.das.ubp.edu.ar/" id="transaccionws"
		implementor="ar.edu.ubp.das.src.comercio.ws.TransaccionWS"
		wsdlLocation="wsdl/transaccionws.wsdl"
		endpointName="tns:TransaccionWSPort"
		serviceName="tns:TransaccionWSService" address="/TransaccionWSPort">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
</beans>
