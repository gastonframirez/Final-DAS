<?xml version="1.0" encoding="UTF-8"?>
<actions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	     xsi:noNamespaceSchemaLocation="../schema/actions.xsd">
    <form-beans>
    </form-beans>
    <action-mappings>
        <action path="/Home.do" type="HomeAction">
            <forward name="success" path="/productos/home.jsp"/>
        </action>
        <action path="/Productos.do" type="ProductosAction">
            <forward name="success" path="/productos/categoria.jsp"/>
        </action>
        <action path="/Buscar.do" type="BuscarAction">
            <forward name="success" path="/productos/categoria.jsp"/>
        </action>
        <action path="/Redireccionar.do" type="RedirectingAction">
            <forward name="success" path="/productos/redirecting.jsp"/>
        </action>
        <action path="/Mensaje.do" type="MessageAction">
            <forward name="success" path="/productos/mensaje.jsp"/>
        </action>
        
    </action-mappings>
</actions>
